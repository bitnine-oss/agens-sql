"    </gml:exterior>\n"
"  </gml:PolygonPatch>\n"
"  <gml:PolygonPatch>\n"
"    <gml:exterior>\n"
"            <gml:LinearRing><gml:posList srsDimension=\"3\">0 1 0 0 1 1 1 1 "
